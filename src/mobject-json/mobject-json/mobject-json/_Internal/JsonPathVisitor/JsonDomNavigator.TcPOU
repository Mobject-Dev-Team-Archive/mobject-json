        </Implementation>
      </Get>
    </Property>
    <Method Name="EndOfParse" Id="{1933187d-055b-4280-a3dd-fb95e40be1c4}">
      <Declaration><![CDATA[METHOD EndOfParse
VAR_INPUT
END_VAR
]]></Declaration>
