    <Method Name="ArrayIndexFound" Id="{98db7489-e8e0-4d0a-a655-8d7a75b6cd7b}">
      <Declaration><![CDATA[METHOD ArrayIndexFound
VAR_INPUT
	Index : UDINT;
END_VAR
]]></Declaration>
